{
  "name": "@mmontes11/iot-client",
  "version": "1.1.0",
  "description": "ES6 client library for consuming IoT server REST API",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "install:submodules": "git submodule foreach npm install",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 10000 test/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run test && npm run clean",
    "build": "babel -d dist/ src/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmontes11/iot-client"
  },
  "keywords": [
    "js",
    "javacript",
    "es6",
    "babel",
    "nodejs",
    "npm",
    "mocha",
    "chai",
    "istanbul",
    "iot-client",
    "iot",
    "internet-of-things",
    "iot-client"
  ],
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "bugs": {
    "url": "https://github.com/mmontes11/iot-client/issues"
  },
  "homepage": "https://github.com/mmontes11/iot-client#readme",
  "dependencies": {
    "http-status": "^1.0.1",
    "node-persist": "^2.1.0",
    "restler": "^3.4.0",
    "underscore": "^1.9.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint-config-mmontes11-node": "^1.0.4",
    "iot-server": "file:./test/lib/iot-server",
    "mocha": "^3.5.0",
    "nyc": "^11.7.1"
  }
}
